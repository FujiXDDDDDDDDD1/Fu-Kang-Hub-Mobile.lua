Dew.newToggle("Toggle JumpPower", "Fu Kang Hub", false, function(toggleState)
    if toggleState then
        game:GetService('Players').LocalPlayer.Character.Humanoid.JumpPower = 500
    else
        game:GetService('Players').LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

Dew.newToggle("Toggle Hyperspeed", "Fu Kang Hub", false, function(toggleState)
    if toggleState then
        _G.WS = "300";
				local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
				Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
				Humanoid.WalkSpeed = _G.WS;
				end)
				Humanoid.WalkSpeed = _G.WS;
    else
        _G.WS = "16";
				local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid;
				Humanoid:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
				Humanoid.WalkSpeed = _G.WS;
				end)
				Humanoid.WalkSpeed = _G.WS;
    end
end)

Dew.newButton("Noclip", "Fu Kang Hub", false,  function(toggleState)
    local Workspace = game:GetService("Workspace")
local Players = game:GetService("Players")

local Plr = Players.LocalPlayer
local Clipon = false

-- Function to toggle noclip
local function ToggleNoclip()
    if Clipon then
        Clipon = false
        print("Noclip is now off")
    else
        Clipon = true
        print("Noclip is now on")
        -- Enable noclip behavior
        game:GetService("RunService").Stepped:Connect(function()
            if Clipon then
                for _, part in pairs(Workspace:GetDescendants()) do
                    if part:IsA("BasePart") and part.Parent == Plr.Character then
                        part.CanCollide = false
                    end
                end
            end
        end)
    end
end

-- Call the function to toggle noclip when needed
ToggleNoclip()

end)

Dew.newButton("Hitbox", "Fu Kang Hub", function()
    Dew.newButton("Hitbox", "Fu Kang Hub", false, function(toggleState)
        _G.HeadSize = 20
        _G.Disabled = true
    
        -- ฟังก์ชันสำหรับเปิดหรือปิดฟังก์ชัน
        local function ToggleFunction()
            _G.Disabled = not _G.Disabled
            if _G.Disabled then
                print("Function Disabled")
            else
                print("Function Enabled")
            end
        end
    
        game:GetService('RunService').RenderStepped:Connect(function()
            if _G.Disabled then
                for i, v in next, game:GetService('Players'):GetPlayers() do
                    if v.Name ~= game:GetService('Players').LocalPlayer.Name then
                        pcall(function()
                            v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
                            v.Character.HumanoidRootPart.Transparency = 0.7
                            v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Black")
                            v.Character.HumanoidRootPart.Material = "Neon"
                            v.Character.HumanoidRootPart.CanCollide = false
                        end)
                    end
                end
            end
        end)
    
        if toggleState == false then
            ToggleFunction()
        else
            -- สำหรับกรณีที่มีการกดปุ่มอื่นหรือสถานะอื่น ๆ ที่คุณต้องการให้ทำอะไรบางอย่างในกรณีนี้
            -- ให้เพิ่มโค้ดที่นี่
        end
    end)
end)
